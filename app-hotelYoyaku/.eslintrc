{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": [
      "plugin:react/recommended",
      "airbnb-base"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "ignorePatterns": ["**/dist/*/*.js", "**/vendor/*.js"],
  "plugins": [
      "react"
  ],
  "rules": {
      "react/react-in-jsx-scope" : 0,
      "react/prop-types" : 0,
      "require-jsdoc" : 0,
      "react/jsx-filename-extension": [0],
      "no-undef" : 0,
      "no-restricted-syntax" : 0
  }
}
